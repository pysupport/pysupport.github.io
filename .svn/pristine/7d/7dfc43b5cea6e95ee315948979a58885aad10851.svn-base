            <!-- CONTACT START -->
            <section id="contact">
                <div class="container" id="contact-details-wrapper">
                    <div class="row">
                        <div class="col-xs-12">
                            <p class="fancy"><span></span></p>
                        </div>
                    </div>
                    <div id="contact-details">
                        <div class="row">
                            <div class="desc col-xs-12 col-sm-6 col-sm-offset-3 intro">
                                <h2>We'd <i class="ion-ios7-heart-outline"></i> to help!</h2>
                                <p>We like to create things with fun, open-minded people. Feel free to say hello!</p>
                            </div>
                        </div>
                        <!-- CONTACT HAS 2 OPTIONS: Form based (option 1) and Address/Phone/Social (option 2) -->
                        <div class="row">
                            <!-- CONTACT OPTION 1 START -->
                            <div class="option col-xs-10 col-xs-offset-1 col-sm-5 col-sm-offset-1">
                                <!-- CONTACT FORM START -->
                                <form role="form" id="contact-form">
                                    <div class="form-group">
                                        <input type="text" class="form-control" name="name" id="name" placeholder="Name">
                                    </div>
                                    <div class="form-group">
                                        <input type="email" class="form-control" name="email" id="email" placeholder="Email">
                                    </div>
                                    <div class="form-group">
                                        <textarea class="form-control" rows="8" name="message" id="message" placeholder="Message"></textarea>
                                    </div>
                                    <div class="form-group">
                                        <a class="btn btn-orange btn-half-block" id="submit-contact-form">Send</a>
                                    </div>
                                </form>
                                <!-- CONTACT FORM END -->
                                <div class="alert alert-success alert-small" id="contact-success" style="display: none;"><i class="ion-checkmark-round"></i> <span>Message sent.</span></div>
                                <div class="alert alert-danger alert-small" id="contact-error" style="display: none;"><i class="ion-close-round"></i> <span>Something is not working. Please try again.</span></div>
                            </div>
                            <!-- CONTACT OPTION 1 END -->
                            <!-- CONTACT OPTION 2 START -->
                            <div class="option col-xs-10 col-xs-offset-1 col-sm-4 col-sm-offset-1">
                                <div class="media">
                                    <i class="pull-left ion-ios7-location-outline fa-2x text-muted"></i>
                                    <div class="media-body">
                                        <address>
                                            <strong>MangoSoft Apps Inc.</strong><br>
                                            #70 Bayan bayanan Ave, Concepcion Uno<br>
                                            Marikina City, Metro Manila<br>
                                            Philippines
                                        </address>
                                    </div>
                                </div>
                                <div class="media">
                                    <i class="pull-left ion-ios7-telephone-outline fa-2x text-muted"></i>
                                    <div class="media-body">
                                        (02) 941 1651
                                    </div>
                                </div>
                                <div class="media">
                                    <i class="pull-left ion-ios7-email-outline fa-2x text-muted"></i>
                                    <div class="media-body">
                                        <a>mangosoftapps@gmail.com</a>
                                    </div>
                                </div>
                                <div class="media">
                                    <div class="media-body">
                                        <hr>
                                        <ul class="list-inline">
                                            <li><a href="https://twitter.com/mangosoftapps" target="_blank"><i class="ion-social-twitter fa-2x"></i></a></li>
                                            <li><a href="https://www.facebook.com/mangosoftappsinc/" target="_blank"><i class="ion-social-facebook fa-2x"></i></a></li>
                                            <li><a href="https://www.pinterest.com/mangosoft/" target="_blank"><i class="ion-social-pinterest fa-2x"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
